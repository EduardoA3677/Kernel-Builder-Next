
[
    {
        "kernelSource": { 
            "name": "Google-Pixel-7-Pro-Radioactive-aosp", 
            "repo": "https://github.com/EduardoA3677/RadioactiveKernel_Pantah.git", 
            "branch": "a15-susfsnext", 
            "device": "gs201",
            "clangname": "AOSP",
            "kernelname": "Radioactive",
            "androidversion": "A15",
            "defconfig": "radioactive_defconfig" 
          },
          "toolchains": [ 
            {  
               "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/master/clang-r522817.tar.gz",  
               "name": "clang",  
               "binaryEnv": [
                "bin"
            ]  
            },
          {
            "url": "https://github.com/mvaisakh/gcc-build/releases/download/31032024/eva-gcc-arm64-31032024.xz",
            "name": "gcc-arm64",
            "binaryEnv": [
                "gcc-arm64/bin"
            ]
          }
          ], 
          "enableCcache": false, 
          "params": { 
            "ARCH": "arm64", 
            "SUBARCH": "arm64",
            "CC": "clang/bin/clang",
            "CXX": "clang/bin/clang++",
            "CLANG_TRIPLE": "aarch64-linux-gnu-",
            "LLVM": "1",
            "LLVM_IAS": "1",
            "LTO": "thin",
            "CC_COMPAT": "",
            "externalCommands": { 
              "CROSS_COMPILE": "gcc-arm64/gcc-arm64/gcc-arm64/bin/aarch64-linux-gnu-",
              "AS": "clang/bin/llvm-as",
              "AR": "clang/bin/llvm-ar",
              "NM": "clang/bin/llvm-nm",
              "OBJCOPY": "clang/bin/llvm-objcopy",
              "OBJDUMP": "clang/bin/llvm-objdump",
              "STRIP": "clang/bin/llvm-strip",
              "LD": "clang/bin/ld.lld",
              "READELF": "clang/bin/llvm-readelf",
              "OBJSIZE": "clang/bin/llvm-size",
              "HOSTCC": "gcc-arm64/gcc-arm64/gcc-arm64/bin/gcc",
              "HOSTCXX": "gcc-arm64/gcc-arm64/gcc-arm64/bin/g++",
              "HOSTAR": "clang/bin/llvm-ar",
              "HOSTLD": "clang/bin/ld.lld"
            } 
          }, 
          "AnyKernel3": { 
            "use": true, 
            "release": true,
            "custom": {
              "repo": "https://github.com/EduardoA3677/AnyKernel3.git",
              "branch": "15.0.0-sultan-gs201"
            }
          },
          "enableKernelSU": false, 
          "KernelSUSFS":{
            "enable": false,
            "branches": "gki-android13-5.10"
          },
          "enableLXC": true        
    }
]
