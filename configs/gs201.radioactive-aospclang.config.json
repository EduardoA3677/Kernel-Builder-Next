
[
    {
        "kernelSource": { 
            "name": "Google-Pixel-7-Pro-Radioactive-aosp", 
            "repo": "https://github.com/EduardoA3677/RadioactiveKernel_Pantah.git", 
            "branch": "a15-susfsnext", 
            "device": "gs201",
            "clangname": "AOSP",
            "kernelname": "Radioactive",
            "androidversion": "A15",
            "defconfig": "radioactive_defconfig" 
          },
          "toolchains": [ 
            {  
               "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/master/clang-r522817.tar.gz",  
               "name": "clang",  
               "binaryEnv": [
                "bin"
            ]  
            }
          ], 
          "enableCcache": false, 
          "params": { 
            "ARCH": "arm64", 
            "SUBARCH": "arm64",
            "CC": "clang/bin/clang",
            "CXX": "clang/bin/clang++",
            "CLANG_TRIPLE": "aarch64-linux-gnu-",
            "LLVM": "1",
            "LLVM_IAS": "1",
            "LTO": "thin",
            "CC_COMPAT": "",
            "externalCommands": { 
              "CROSS_COMPILE": "aarch64-linux-gnu-",
                make -j$(nproc --all) \
    ARCH=arm64 \
    O=out \
    CC=clang \
    CROSS_COMPILE="$HOME/gcc-64/bin/${GCC64}-" \
    AR=llvm-ar \
    AS=llvm-as \
    NM=llvm-nm \
    STRIP=llvm-strip \
    OBJCOPY=llvm-objcopy \
    OBJDUMP=llvm-objdump \
    OBJSIZE=llvm-size \
    READELF=llvm-readelf \
    HOSTCC=clang \
    HOSTCXX=clang++ \
    HOSTAR=llvm-ar \
    CLANG_TRIPLE="aarch64-linux-gnu-" \
    LLVM_IAS=1 \
    LLVM=1 \
    LD=ld.lld \
    LD_LIBRARY_PATH=$HOME/clang/lib \
    LTO_CLANG=1 \
    KBUILD_LTO=thin \
    radioactive_defconfig all
                
              "AS": "clang/bin/llvm-as",
              "AR": "clang/bin/llvm-ar",
              "NM": "clang/bin/llvm-nm",
              "OBJCOPY": "clang/bin/llvm-objcopy",
              "OBJDUMP": "clang/bin/llvm-objdump",
              "STRIP": "clang/bin/llvm-strip",
              "LD": "clang/bin/ld.lld",
              "READELF": "clang/bin/llvm-readelf",
              "OBJSIZE": "clang/bin/llvm-size",
            } 
          }, 
          "AnyKernel3": { 
            "use": true, 
            "release": true,
            "custom": {
              "repo": "https://github.com/EduardoA3677/AnyKernel3.git",
              "branch": "15.0.0-sultan-gs201"
            }
          },
          "enableKernelSU": false, 
          "KernelSUSFS":{
            "enable": false,
            "branches": "gki-android13-5.10"
          },
          "enableLXC": true        
    }
]
