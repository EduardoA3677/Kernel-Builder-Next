[
    {
        "kernelSource": { 
            "name": "P7P-Radioactibe", 
            "repo": "https://github.com/EduardoA3677/RadioactiveKernel_Pantah", 
            "branch": "A15-stable", 
            "device": "gs201", 
            "defconfig": "radioactive_defconfig" 
          }, 
          "toolchains": [ 
            {  
               "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/master/clang-r522817.tar.gz",  
               "name": "clang",  
               "binaryEnv": [
                "bin"
            ]  
            } 
          ], 
          "enableCcache": true, 
          "params": { 
            "ARCH": "arm64", 
            "CC": "clang/bin/clang", 
            "externalCommands": { 
              "CROSS_COMPILE": "aarch64-linux-gnu-", 
              "CLANG_TRIPLE": "tools/bin/aarch64-linux-gnu-",
              "AS": "clang/bin/llvm-as", 
              "AR": "clang/bin/llvm-ar",  
              "NM":"clang/bin/llvm-nm" ,  
              "OBJCOPY":"clang/bin/llvm-objcopy",  
              "OBJDUMP":"clang/bin/llvm-objdump",  
              "STRIP":"clang/bin/llvm-strip",  
              "LD":"clang/bin/ld.lld", 
              "LTO": "thin" 
            } 
          }, 
          "AnyKernel3": { 
            "use": true, 
            "release": true,
            "custom": {
              "repo": "https://github.com/TheWildJames/AnyKernel3",
              "branch": "15.0.0-sultan-gs201"
            }
          },
          "enableKernelSU": false, 
          "KernelSUSFS":{
            "enable": false,
            "branches": "gki-android13-5.10"
          },
          "enableLXC": false        
    }
]
