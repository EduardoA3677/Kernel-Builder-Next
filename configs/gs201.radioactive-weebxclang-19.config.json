[
    {
        "kernelSource": { 
            "name": "Google-Pixel-7-Pro-Radioactive-WeebX", 
            "repo": "https://github.com/EduardoA3677/RadioactiveKernel_Pantah.git", 
            "branch": "a15-susfsnext",
            "device": "gs201",
            "clangname": "WeebX",
            "kernelname": "Radioactive",
            "androidversion": "A15",
            "defconfig": "radioactive_defconfig" 
          }, 
          "toolchains": [ 
            {  
               "url": "https://github.com/XSans0/WeebX-Clang/releases/download/WeebX-Clang-19.1.5-release/WeebX-Clang-19.1.5.tar.gz",  
               "name": "clang",  
               "binaryEnv": [
                "bin"
            ]  
            }, 
          {
            "repo": "https://github.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9.git",
            "branch": "lineage-19.1",
            "name": "gcc",
            "binaryEnv": [
                "bin"
            ]
          }
          ], 
          "enableCcache": false, 
          "params": { 
            "ARCH": "arm64", 
            "SUBARCH": "arm64",
            "CC": "clang/bin/clang",
            "CXX": "clang/bin/clang++",
            "CLANG_TRIPLE": "aarch64-linux-gnu-",
            "LLVM": "1",
            "LLVM_IAS": "1",
            "LTO": "thin",
            "CC_COMPAT": "",
            "externalCommands": { 
              "CROSS_COMPILE": "gcc/bin/aarch64-linux-android-gnu-",
              "AS": "clang/bin/llvm-as",
              "AR": "clang/bin/llvm-ar",
              "NM": "clang/bin/llvm-nm",
              "OBJCOPY": "clang/bin/llvm-objcopy",
              "OBJDUMP": "clang/bin/llvm-objdump",
              "STRIP": "clang/bin/llvm-strip",
              "LD": "clang/bin/ld.lld",
              "READELF": "clang/bin/llvm-readelf",
              "OBJSIZE": "clang/bin/llvm-size",
              "HOSTCC": "gcc/bin/aarch64-linux-android-gcc",
              "HOSTCXX": "gcc/bin/aarch64-linux-android-g++",
              "HOSTAR": "gcc/bin/aarch64-linux-android-ar",
              "HOSTLD": "gcc/bin/aarch64-linux-android-ld"
            } 
          }, 
          "AnyKernel3": { 
            "use": true, 
            "release": true,
            "custom": {
              "repo": "https://github.com/EduardoA3677/AnyKernel3.git",
              "branch": "15.0.0-sultan-gs201"
            }
          },
          "enableKernelSU": false, 
          "KernelSUSFS":{
            "enable": false,
            "branches": "gki-android13-5.10"
          },
          "enableLXC": true        
    }
]
